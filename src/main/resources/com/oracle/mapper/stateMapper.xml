<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oracle.dao.StateDao">


	<select id="getStateByType" resultType="state" parameterType="int">
		select * from tb_state   
		<where>
			<if test="type!=0">
			    type=#{type} 
			</if>
		</where>
		order by stateId
	</select>


	
	

</mapper>